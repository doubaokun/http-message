{
    "name": "hyperf/http-message",
    "type": "library",
    "description": "microservice framework base on swoole",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "http-message"
    ],
    "require": {
        "hyperf/utils": "~1.1.0",
        "psr/http-message": "^1.0",
        "laminas/laminas-mime": "^2.7"
    },
    "suggest": {
        "psr/container": "Required to replace RequestParserInterface."
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\HttpMessage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\HttpMessage\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\HttpMessage\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
